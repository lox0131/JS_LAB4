{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "strictNullChecks": false,
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/Middleware/db.js", "src/pages/signin.jsx", "src/Components/Navbar.jsx", "src/Components/Logo.jsx", "src/Components/MethodsSide.jsx", "src/pages/index.tsx", "src/pages/personalMethods.jsx"],
  "exclude": ["node_modules"]
}
